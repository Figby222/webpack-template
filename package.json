{
  "name": "webpack-template",
  "type": "module",
  "version": "1.0.0",
  "description": "A repository template that uses NPM webpack along with other useful plugins to aid the coding process",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "webpack --config webpack.prod.cjs",
    "start": "webpack serve --open --config webpack.dev.cjs",
    "deploy": "git subtree push --prefix dist origin gh-pages"
  },
  "author": "Ryan Posey",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.24.3",
    "@babel/plugin-transform-runtime": "^7.24.0",
    "@babel/preset-env": "^7.24.3",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.11.0",
    "csv-loader": "^3.0.5",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "html-loader": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "3.2.5",
    "style-loader": "^3.3.4",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.2",
    "xml-loader": "^1.2.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.24.0",
    "css-minimizer-webpack-plugin": "^6.0.0",
