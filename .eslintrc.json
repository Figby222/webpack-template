{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "quotes": [
            "error",
            "double"
        ],
        "prefer-arrow-callback": "off",
        "func-names": "off",
        "no-use-before-define": "off",
        "no-unused-vars": "off",
        "no-underscore-dangle": "off",
        "prefer-const": "off",
        "no-plusplus": "off",
        "eslint-plugin-import/extensions": [
            "never"
        ]
    }
}
